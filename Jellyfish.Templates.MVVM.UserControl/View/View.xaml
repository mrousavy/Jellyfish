<UserControl
    x:Class="$rootnamespace$.$safeitemrootname$"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:$rootnamespace$"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DataContext="{d:DesignInstance Type=$safeitemrootname$Model,
                                     IsDesignTimeCreatable=True}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    DataContext="{Binding $safeitemrootname$, Source={StaticResource ViewModelLocator}}"
    mc:Ignorable="d">
    <Grid>

    </Grid>
</UserControl>
